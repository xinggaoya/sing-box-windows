{
  "name": "sing-box-windows",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint:oxlint": "oxlint . --fix -D correctness",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "tauri": "tauri",
    "tauri dev": "tauri dev",
    "tauri build": "tauri build",
    "tauri build:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "tauri build:linux:deb": "tauri build --target x86_64-unknown-linux-gnu -- --bundle deb",
    "tauri build:linux:appimage": "tauri build --target x86_64-unknown-linux-gnu -- --bundle appimage",
    "tauri build:windows": "tauri build --target x86_64-pc-windows-msi",
    "tauri build:all": "pnpm tauri build:windows && pnpm tauri build:linux:deb && pnpm tauri build:linux:appimage"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-autostart": "^2.5.0",
    "@tauri-apps/plugin-store": "~2.2.1",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "@tauri-apps/plugin-websocket": "~2.3.1",
    "@tauri-apps/plugin-window-state": "^2.4.0",
    "@vicons/antd": "^0.13.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.13.0",
    "@vueuse/core": "^12.8.2",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.22",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.8.4",
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^20.19.19",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.36.0",
    "eslint-plugin-oxlint": "^0.11.1",
    "eslint-plugin-vue": "^9.33.0",
    "mitt": "^3.0.1",
    "naive-ui": "^2.43.1",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.11.1",
    "prettier": "^3.6.2",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "vfonts": "^0.0.3",
    "vite": "^6.3.6",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.12"
  }
}